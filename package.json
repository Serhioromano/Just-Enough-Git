  "version": "0.1",
  "repository": {
      "url": "https://github.com/Xuangeaha/Just-Enough-Git.git"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Extension Packs"
  ],
  "extensionPack": [ 
